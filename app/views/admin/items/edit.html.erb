<h1>商品編集</h1>
<%= form_with model: @item, url: admin_update_items_path do |f| %>
  商品画像
  <%= f.file_field :image, accept: "image/*" %><br>
  商品名
  <%= f.text_field :name %><br>
  商品説明文
  <%= f.text_field :introduction %><br>
  
  税抜価格
  <%= f.text_field :price %><br>
  
  <%= f.submit '変更を保存' %>
<% end %>

